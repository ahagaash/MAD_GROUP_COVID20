<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Question">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="366dp"
        android:layout_height="66dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="61dp"
        android:text="@string/subheadingq1"
        android:textColor="#100101"
        android:textSize="24.5sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="348dp"
        android:layout_height="192dp"
        android:layout_marginTop="45dp"
        android:layout_marginBottom="330dp"
        android:text="@string/para1q1"
        android:textAlignment="textStart"
        android:textColor="#635B5B"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.423"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/Cardno"
        android:layout_width="348dp"
        android:layout_height="192dp"
        android:layout_marginBottom="122dp"
        android:text="@string/para2q1"
        android:textAlignment="textStart"
        android:textColor="#635B5B"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="339dp"
        android:layout_height="82dp"
        android:layout_marginBottom="76dp"
        android:text="@string/para3q1"
        android:textAlignment="textStart"
        android:textColor="#635B5B"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.43"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Cardno"
        app:layout_constraintVertical_bias="0.666"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>